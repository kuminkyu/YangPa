<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MypageMapper">


	<select id = "mylist" parameterType="java.lang.String" resultType="co.kr.yangpa.board.BoardDTO">
		select bno, state, title, write_date, substr(type,1,1) as type
		from board
		where writer = #{mno}
	</select>
	
</mapper>









